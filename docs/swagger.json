    "info": {
        "description": "The ETSI MEC ISG MEC013 Location API described using OpenAPI",
        "title": "ETSI GS MEC 013 - Location API",
        "contact": {
            "name": "Iulisloi Zacarias",
            "url": "https://github.com/izacarias"
        },
        "license": {
            "name": "Apache 2.0",
